    "babel-jest": "26.6.3",
    "concurrently": "^7.2.2",
    "eslint": "7.32.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.67.0",
    "postcss": "^8.4.14",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.1.4"
  },
  "jest": {
